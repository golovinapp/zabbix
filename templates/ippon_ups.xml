<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
  <version>5.0</version>
  <date>2025-09-07T11:48:42Z</date>
  <groups>
    <group>
      <name>UPS</name>
    </group>
  </groups>
  <templates>
    <template>
      <template>Template_SNMP_UPS_Ippon</template>
      <name>Template SNMP UPS Ippon (WebPower Smart II)</name>
      <groups>
        <group>
          <name>UPS</name>
        </group>
      </groups>
      <applications>
        <application><name>SNMP UPS</name></application>
      </applications>
      <items>
        <item>
          <name>UPS model</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>iso.3.6.1.2.1.33.1.1.4.0</snmp_oid>
          <key>ups.model</key>
          <value_type>CHAR</value_type>
          <applications>
            <application>
              <name>SNMP UPS</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Battery status</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>iso.3.6.1.2.1.33.1.2.1.0</snmp_oid>
          <key>ups.battery.status</key>
          <value_type>FLOAT</value_type>
          <applications>
            <application>
              <name>SNMP UPS</name>
            </application>
          </applications>
        <triggers>
          <trigger>
            <expression>{last(0)}&lt;&gt;2</expression>
            <name>Battery problem on {HOST.NAME}</name>
            <priority>DISASTER</priority>
            <manual_close>YES</manual_close>
          </trigger>
        </triggers>
        </item>
        <item>
          <name>Battery charge (%)</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>iso.3.6.1.2.1.33.1.2.4.0</snmp_oid>
          <key>ups.battery.charge</key>
          <value_type>FLOAT</value_type>
          <units>%</units>
          <applications>
            <application>
              <name>SNMP UPS</name>
            </application>
          </applications>
        <triggers>
          <trigger>
            <expression>{last(0)}&lt;20</expression>
            <name>Battery low charge on {HOST.NAME}</name>
            <priority>HIGH</priority>
            <manual_close>YES</manual_close>
          </trigger>
        </triggers>
        </item>
        <item>
          <name>Battery runtime remaining (s)</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>iso.3.6.1.2.1.33.1.2.3.0</snmp_oid>
          <key>ups.battery.runtime</key>
          <value_type>FLOAT</value_type>
          <units>s</units>
          <applications>
            <application>
              <name>SNMP UPS</name>
            </application>
          </applications>
        <triggers>
          <trigger>
            <expression>{last(0)}&lt;300</expression>
            <name>Battery runtime less than 5 min on {HOST.NAME}</name>
            <priority>DISASTER</priority>
            <manual_close>YES</manual_close>
          </trigger>
        </triggers>
        </item>
        <item>
          <name>Input voltage</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>iso.3.6.1.2.1.33.1.3.3.1.3.1</snmp_oid>
          <key>ups.input.voltage</key>
          <value_type>FLOAT</value_type>
          <units>V</units>
          <applications>
            <application>
              <name>SNMP UPS</name>
            </application>
          </applications>
        <triggers>
          <trigger>
            <expression>{last(0)}&lt;200</expression>
            <name>Input voltage low on {HOST.NAME}</name>
            <priority>AVERAGE</priority>
            <manual_close>YES</manual_close>
          </trigger>
        </triggers>
        </item>
        <item>
          <name>Output voltage</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>iso.3.6.1.2.1.33.1.4.4.1.2.1</snmp_oid>
          <key>ups.output.voltage</key>
          <value_type>FLOAT</value_type>
          <units>V</units>
          <applications>
            <application>
              <name>SNMP UPS</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Output load (%)</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>iso.3.6.1.2.1.33.1.4.4.1.5.1</snmp_oid>
          <key>ups.output.load</key>
          <value_type>FLOAT</value_type>
          <units>%</units>
          <applications>
            <application>
              <name>SNMP UPS</name>
            </application>
          </applications>
        <triggers>
          <trigger>
            <expression>{last(0)}&gt;90</expression>
            <name>UPS overload on {HOST.NAME}</name>
            <priority>HIGH</priority>
            <manual_close>YES</manual_close>
          </trigger>
        </triggers>
        </item>
        <item>
          <name>Temperature (°C)</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>iso.3.6.1.2.1.33.1.2.7.0</snmp_oid>
          <key>ups.temperature</key>
          <value_type>FLOAT</value_type>
          <units>°C</units>
          <applications>
            <application>
              <name>SNMP UPS</name>
            </application>
          </applications>
        <triggers>
          <trigger>
            <expression>{last(0)}&gt;50</expression>
            <name>UPS high temperature on {HOST.NAME}</name>
            <priority>AVERAGE</priority>
            <manual_close>YES</manual_close>
          </trigger>
        </triggers>
        </item>
      </items>
    </template>
  </templates>
</zabbix_export>
